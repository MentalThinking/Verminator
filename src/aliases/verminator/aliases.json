[
  {
    "name": "verminate",
    "regex": "^(verminate|vstart)$",
    "script": "Verminator.start()"
  },
  {
    "name": "unverminate",
    "regex": "^(unverminate|vstop)$",
    "script": "Verminator.stop()"
  },
  {
    "name": "verminpause",
    "regex": "^(verminpause|vp)$",
    "script": "Verminator.pause()"
  },
  {
    "name": "vermincont",
    "regex": "^(vermincont|vc)$",
    "script": "Verminator.continue()"
  },
  {
    "name": "verminconfig",
    "regex": "^verminconfig (.+)"
  },
  {
    "name": "verminshow",
    "regex": "^(verminshow|vshow)$",
    "script": "Verminator.container:show()"
  },
  {
    "name": "verminhide",
    "regex": "^(verminhide|vhide)$",
    "script": "Verminator.container:hide()"
  }
]